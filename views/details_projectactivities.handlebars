<table class="table">
    <thead>
        <tr>
            <th>Activity Description</th>
            <th>Target Start Date</th>
            <th>Actual Start Date</th>
            <th>Target Completion Date</th>
            <th>Actual Completion Date</th>
            <th>Progress</th>
            <th>Activity Notes</th>
        </tr>
    </thead>
    <tbody>
           
        {{#each results}}
        <tr>
            <td>{{activityDescription}}</td>
            <td>{{targetStartDate}}</td>
            <td>{{actualStartDate}}</td>
            <td>{{targetCompletionDate}}</td>
            <td>{{actualCompletionDate}}</td>
            <td>{{progress}}</td>
            <td>{{activityNotes}}</td>
        </tr>
        {{/each}}
    </tbody>
</table>